{
  "name": "magic-string-ast",
  "version": "0.3.0",
  "packageManager": "pnpm@8.10.4",
  "description": "Extend Babel AST for magic-string.",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/magic-string-ast#readme",
  "bugs": {
    "url": "https://github.com/sxzz/magic-string-ast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/magic-string-ast.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.ts",
        "import": "./dist/index.d.mts"
      },
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "magic-string": "^0.30.5"
  },
  "devDependencies": {
    "@babel/types": "^7.23.3",
    "@sxzz/eslint-config": "^3.7.4",
    "@sxzz/prettier-config": "^1.0.4",
    "@types/node": "^20.9.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
