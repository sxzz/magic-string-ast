{
  "name": "magic-string-ast",
  "version": "0.4.1",
  "packageManager": "pnpm@9.0.1",
  "description": "Extend Babel AST for magic-string.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/magic-string-ast#readme",
  "bugs": {
    "url": "https://github.com/sxzz/magic-string-ast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/magic-string-ast.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "magic-string": "^0.30.9"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@sxzz/eslint-config": "^3.9.1",
    "@sxzz/prettier-config": "^2.0.1",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
